@model NetModelsLibrary.Models.AuthModel
<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>UserChat</title>
</head>
<body>

<div class="row">
  	<div>
    	<h2>Ауторизація</h2>
    	<form asp-action="Authorization" asp-controller="Home" method="post">
			<div>
				<label asp-for="Login">Логін</label>
				<input asp-for="Login"/>
			</div>
			<div>
				<label asp-for="PasswordMD5">Пароль</label>
				<input type="password" asp-for="PasswordMD5"/>
			</div>
			<div>
				<input type="submit" value="Ауторизація"/>
			</div>
		</form>
  	</div>
</div>

</body>
</html>

